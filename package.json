{
  "name": "bun-svelte-kit",
  "type": "module",
  "scripts": {
    "dev": "bun --bun run vite dev",
    "build": "bun --bun run vite build",
    "start": "cd build && bun start",
    "format": "bun --bun run prettier --write ./src",
    "lint": "bun --bun run eslint ./src"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@sveltejs/kit": "^2.16.1",
    "@tailwindcss/vite": "^4.0.0",
    "@types/bun": "latest",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-svelte": "^2.46.1",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "svelte": "^5.19.4",
    "svelte-adapter-bun": "^0.5.2",
    "tailwindcss": "^4.0.0",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.0.11"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  }
}
